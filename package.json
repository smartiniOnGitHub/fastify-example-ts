{
  "name": "fastify-example-ts",
  "version": "1.0.0",
  "description": "Example web application with Fastify, made in TypeScript",
  "main": "dist/server",
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir ./dist",
    "build:watch": "tsc -p tsconfig.json --watch --outDir ./dist",
    "cache:cleanup": "npm cache clean --force",
    "cache:verify": "npm cache verify",
    "clean": "rm -rf ./dist/*",
    "clean:install": "rm -rf ./package-lock.json ./node_modules/",
    "dependency:log": "npm list > ./temp/dependencies.log",
    "postinstall": "",
    "prestart": "",
    "start": "node ./dist/server",
    "start:simple": "node ./dist/server-simple"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "fast-json-stringify": "^2.2.3",
    "fastify": "^3.1.1",
    "fastify-static": "^3.2.0",
    "is-docker": "^2.0.0",
    "make-promises-safe": "^5.1.0",
    "point-of-view": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "cross-env": "^7.0.2",
    "simple-get": "^4.0.0",
    "standard": "^14.3.4",
    "standardx": "^5.0.0",
    "tap": "^14.10.8",
    "tsd": "^0.13.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10.13.0"
  },
  "tsd": {
    "directory": "types",
    "compilerOptions": {
      "esModuleInterop": true,
      "strict": true
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartiniOnGitHub/fastify-example-ts.git"
  },
  "keywords": [
    "node",
    "example",
    "fastify",
    "ejs",
    "es6",
    "es2017",
    "typescript"
  ],
  "author": "Sandro Martini <sandro.martini@gmail.com>",
  "license": "Apache-2.0"
}
